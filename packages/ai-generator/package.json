{
  "name": "@raynaui/ai-generator",
  "version": "1.0.0",
  "description": "AI-powered component generator for RaynaUI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "raynaui-ai": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:dev": "vitest",
    "demo": "tsx demo.ts",
    "format:write": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "dedent": "^1.6.0",
    "fs-extra": "^11.3.1",
    "kleur": "^4.1.5",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "zod": "^3.24.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.27",
    "@types/prompts": "^2.4.9",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.5.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ai",
    "components",
    "generator",
    "raynaui",
    "react",
    "typescript"
  ],
  "author": {
    "name": "RaynaUI",
    "url": "https://twitter.com/raynaui"
  },
  "license": "MIT"
} 